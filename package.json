{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "node scripts/seedDB.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "firebase": "^5.9.2",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "react-firebaseui": "file:node_modules/react-firebaseui",
    "sequelize": "^5.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aimannadzmi/rentdepot.git"
  },
  "bugs": {
    "url": "https://github.com/aimannadzmi/rentdepot/issues"
  },
  "homepage": "https://github.com/aimannadzmi/rentdepot#readme"
}
